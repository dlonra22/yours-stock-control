<h2>All Transactions</h2>
<p><a href="/"><button>Back To Home Page</button></a></p>
<% @transactions.each do |t| %>
<p><a href="/mytransaction/<%=t.id%>"><button>Show Item</button></a></p>
<p><strong>Created_On: </strong><%=t.created_at%> <strong>Qty: </strong><%=t.quantity%></p>
<p><strong>Transaction Notes: </strong><%=t.transaction_notes%></p>
<p><strong>Value: </strong><%=t.value%></p>
    <p><strong>Created_On: </strong><%=t.created_at%> <strong>Item:</strong> <%=item.name%> <strong>Qty: </strong><%=t.quantity%> <strong>Price: </strong><%=item.price%></p>
    <p><strong>By:</strong> <%=@user.name%><p>
    <p></p><strong>Transaction Notes: </strong><%=t.transaction_notes%></p>
<%end%>
